{
  "name": "Pinterest-apollo-graphql",
  "scripts": {
    "start:watch": "nodemon",
    "lint":
      "tslint  --project ./tsconfig.json 'src/**/*.{ts,tsx}' -c tslint.json --fix",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit --useStderr --bail",
    "test": "jest",
    "compile": "tsc",
    "coverage": "jest --coverage",
    "start:prod": "node ./dist/src/server.js",
    "script": "m2gql -d mongodb://localhost:25555/PinterestDB -s fullPinterest",
    "pretty": "pretty-quick"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-subscriptions": "latest",
    "graphql-tag": "^2.7.3",
    "graphql-tools": "^2.21.0",
    "jsonwebtoken": "^8.1.1",
    "m2gql": "^0.0.1",
    "mongodb": "^3.0.2",
    "multer": "^1.3.0",
    "request-promise": "latest",
    "rxjs": "latest",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/glob": "^5.0.35",
    "@types/jest": "^21.1.10",
    "@types/mongodb": "^3.0.5",
    "@types/multer": "^1.3.6",
    "@types/socket.io": "^1.4.32",
    "jest": "^22.4.0",
    "nodemon": "^1.15.0",
    "husky": "^0.14.3",
    "prettier": "~1.12.1",
    "pretty-quick": "^1.4.1",
    "reflect-metadata": "^0.1.12",
    "supertest": "latest",
    "ts-jest": "^21.2.4",
    "ts-node": "^5.0.0",
    "tslint": "~5.7.0",
    "tslint-sonarts": "^1.6.0",
    "typescript": "2.4.2",
    "inversify": "latest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "./e2e/setup.ts",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json"]
  }
}
